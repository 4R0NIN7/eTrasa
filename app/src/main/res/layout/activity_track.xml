<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relLayoutTrack">

    <fragment
        android:id="@+id/mapTrack"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".TrackActivity" />

    <ImageView
        android:id="@+id/endSettingTrack"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="@id/mapTrack"
        android:layout_alignParentRight="true"
        android:layout_marginTop="150dp"
        android:layout_marginRight="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/ic_end_setting_track" />

    <ImageView
        android:id="@+id/settingsDataTrack"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="@id/mapTrack"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="150dp"
        android:layout_marginRight="10dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/ic_settings_data" />

</RelativeLayout>